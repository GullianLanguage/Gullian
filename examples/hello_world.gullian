import std.vec
import std.io
import std.fmt

fun main() : int {
    let fruits = vec.new("apple")

    fruits.push("banana")
    fruits.push("pear")

    let i = 0
    while i < fruits.length {
        io.puts(fruits.items[i])
        i += 1

    }

    let x: int = str
    io.puts(x.to_string())
}
