import std.vec
import std.io
import std.fmt
import std.err

fun main() : int {
    let fruits = vec.new("apple")

    fruits.push("banana")
    fruits.push("pear")

    let i = 0
    while i < fruits.length {
        io.puts(fruits.items[i])
        i += 1

    }

    let x: int = str
    io.puts(x.to_string())

    let y = err.ok(x)

    io.puts(y.to_string())
    io.puts(fruits.has("pear").to_string())
}
