import std.io

enum TokenKind {
    LeftParenthesis,
    RightParenthesis,
    Plus,
    Minus,
    Star,
    Slash,
    Name,
    Bool,
    Int,
    Float,
    String,
}

fun TokenKind.repr(self: TokenKind) : str {
    let value: str = switch self {
        TokenKind.LeftParenthesis: "LeftParenthesis",
        TokenKind.RightParenthesis: "LeftParenthesis",
        TokenKind.Plus: "LeftParenthesis",
        TokenKind.Minus: "LeftParenthesis",
        TokenKind.Star: "LeftParenthesis",
        TokenKind.Slash: "LeftParenthesis",
        TokenKind.Name: "LeftParenthesis",
        TokenKind.Bool: "LeftParenthesis",
        TokenKind.Int: "LeftParenthesis",
        TokenKind.Float: "LeftParenthesis",
        TokenKind.String: "LeftParenthesis",
        _: "..."
    }

    return value
}

struct Token {
    kind: TokenKind,
    begin: u16,
    end: u16
}


fun main() : int {
    let token = TokenKind.LeftParenthesis

    io.puts(token.repr())
}
