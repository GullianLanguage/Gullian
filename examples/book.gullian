import std.io
import std.fmt

struct Book {
    title: str,
    author: str,
    year: int
}

fun main() : int {
    let alice_in_wonderland = Book { "Alice in Wonderland", "Lewis Carroll", 1865 }

    io.puts(alice_in_wonderland.title)
    io.puts(alice_in_wonderland.author)
    io.puts(alice_in_wonderland.year.to_string())
}